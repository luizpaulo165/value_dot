{
  "manifest_version": 3,
  "name": "ValueDot",
  "version": "2.0",
  "description": "Plugin for artist check values of images.",
  "permissions": ["activeTab", "scripting", "tabs", "storage"],
  "host_permissions": ["http://*/", "https://*/"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/grayfy_16.png",
    "32": "icons/grayfy_32.png",
    "48": "icons/grayfy_48.png",
    "128": "icons/grayfy_128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/grayfy_16.png",
      "32": "icons/grayfy_32.png",
      "48": "icons/grayfy_48.png",
      "128": "icons/grayfy_128.png"
    }
  }
}
